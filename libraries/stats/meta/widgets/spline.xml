<?xml version="1.0" encoding="ISO-8859-1"?>
<?xml-stylesheet type="text/xsl" href="../../help.xsl"?>
<documentation><widgetXML>
    <name>Spline Fit</name>
    <icon>stats.png</icon>
    <screenshots></screenshots>
    <summary>Produces a spline fit of X/Y data.</summary>
    <tags>
        <tag priority="10">
            Parametric
        </tag>
    </tags>
    <author>
        <authorname>Red-R Core Development Team</authorname>
        <authorcontact>www.red-r.org</authorcontact>
    </author>
    </widgetXML>
<signals>
<input>
	<name>x</name>
	<description>
X data
	</description>
	<signalClass>base.RVector,base.RDataFrame</signalClass>
</input>
<input>
	<name>y</name>
	<description>
Y data
	</description>
	<signalClass>base.RVector</signalClass>
</input>
<output>
	<name>spline Output</name>
	<description>
spline Output fit
	</description>
	<signalClass>base.RModelFit</signalClass>
</output>
<output>
	<name>spline plot attribute</name>
	<description>
spline plot attribute
	</description>
	<signalClass>plotting.RPlotAttribute</signalClass>
</output>
</signals>
</documentation>